<h1>Song Library</h1>
<div><%= link_to 'Create Song', new_song_path %></div>
<table>
<thead>
<tr>
<th>Title</th>
<th>Artist Name</th>
<th>Release Year</th>
<th>Genre</th>
</tr>
</thead>
<tbody>
<% @songs.each do |song| %>
    <tr>
    <td><%= song.title %></td>
    <td><%= song.artist_name %></td>
    <td><%= song.release_year%></td>
    <td><%= song.genre %><br></td>
    <<td>%= link_to 'View', song %> |
    <%= link_to 'Edit', edit_song_path(song) %> |
    <%= link_to 'Delete', song, method: :destroy, data: { confirm: 'For real?' } %></td>
    </tr>
<% end %>
</tbody>
</table>