<h2>Songs</h2>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Released?</th>
            <th>Released year</th>
            <th>Artist name</th>
            <th>Genre</th>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
        <tr>
            <th><%= link_to song.title, song_path(song) %></th>
            <th><%= song.released %></th>
            <th><%= song.release_year %></th>
            <th><%= song.artist_name %></th>
            <th><%= song.genre %></th>
        </tr>
        <% end %>
    </tbody>
</table>